{
  "lookup_paths": [
    "src"
  ],
  "declaration_keyword": "import",
  "aliases": {
    "FacebookStrategy": "passport-facebook",
    "session": "express-session",
    "React": {
      "path": "react",
      "destructure": [
        "PropTypes"
      ]
    },
    "ReactRedux": {
      "path": "react-redux",
      "destructure": [
        "connect"
      ]
    },
    "ReactRouter": {
      "path": "react-router",
      "destructure": [
        "Link"
      ]
    },
    "Redux": {
      "path": "redux",
      "destructure": [
        "combineReducers"
      ]
    },
    "ReduxAction": {
      "path": "redux-action",
      "destructure": [
        "createAction",
        "handleActions"
      ]
    },
    "ReduxSimpleRouter": {
      "path": "redux-simple-router",
      "destructure": [
        "routeReducer"
      ]
    },
    "styles": "./index.scss"
  }
}
