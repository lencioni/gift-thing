[
  {
    "use_relative_paths": true,
    "eslint_executable": "node_modules/.bin/eslint",
    "aliases": {
      "FacebookStrategy": "passport-facebook",
      "session": "express-session",
      "styles": "./index.scss"
    },
    "named_exports": {
      "react": [
        "PropTypes"
      ],
      "react-redux": [
        "connect"
      ],
      "react-router": [
        "Link"
      ],
      "redux": [
        "combineReducers"
      ],
      "redux-action": [
        "createAction",
        "handleActions"
      ],
      "redux-simple-router": [
        "routeReducer"
      ]
    }
  },
  {
    "applies_to": "src/containers/Dev*",
    "import_dev_dependencies": true
  },
  {
    "applies_to": "server/**",
    "import_dev_dependencies": true
  }
]
